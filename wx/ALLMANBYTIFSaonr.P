PROCEDURE inlas_UI: 

   EMPTY TEMP-TABLE tidinahw NO-ERROR.
   
   INPUT FROM VALUE(filnamn) NO-ECHO.
   REPEAT:
      DO TRANSACTION: 
         CREATE tidinahw.
         ASSIGN.
         IMPORT DELIMITER ";" tidinahw   NO-ERROR.
      END.               
   END.
   FOR EACH tidinahw WHERE tidinahw.AONRDELNR = "":
      DELETE tidinahw.
   END.           
END PROCEDURE.

