DEFINE BUFFER tidbuff FOR TIDREGITAB.

FIND FIRST FORETAG USE-INDEX FORETAG NO-LOCK NO-ERROR.
globforetag = FORETAG.FORETAG.
globanv = "AUTOMAT".
IF FORETAG.PROGRAM = "PLUSD" THEN plusdval = TRUE.
			     ELSE plusdval = FALSE.
idag = TODAY - 1.
regdatum = TODAY - 1.
RUN REGDAG.P.
helg = 0.
IF WEEKDAY(regdatum) = 2 THEN helg = 2.
START:
FOR EACH PERSONALTAB WHERE PERSONALTAB.AKTIV = TRUE
USE-INDEX  PERSONALKOD NO-LOCK:
   persrec = RECID(PERSONALTAB).
   FIND FIRST ANSTFORMTAB WHERE ANSTFORM.ANSTALLNING = PERSONALTAB.ANSTALLNING
   USE-INDEX ANSTF NO-LOCK NO-ERROR.
   FIND FIRST OVERAVTAB WHERE OVERAVTAB.DATUM = regdatum AND
   OVERAVTAB.KOD = ANSTFORMTAB.KOD USE-INDEX ODATUM NO-LOCK NO-ERROR.
   IF NOT AVAILABLE OVERAVTAB THEN DO:
      persrec = persrec.
   END.
   ELSE DO:
      IF OVERAVTAB.EQDAG = 7 OR OVERAVTAB.EQDAG = 1 THEN NEXT START.
      IF OVERAVTAB.DAGEQ = "KLA" THEN NEXT START.
   END.
   RUN REGVEC.P.
   RUN SLUTARB.P.
   IF regstart = regslut THEN NEXT START.
   regdatum3 = regdatum - helg.
   FIND FIRST TIDREGITAB WHERE TIDREGITAB.PERSONALKOD = PERSONALTAB.PERSONALKOD AND
   TIDREGITAB.DATUM = regdatum AND TIDREGITAB.TIDLOG = TRUE AND
   TIDREGITAB.START GE regstart AND TIDREGITAB.SLUT LE regslut
   USE-INDEX PSTART NO-LOCK NO-ERROR.
   IF NOT AVAILABLE TIDREGITAB THEN DO:
      FIND LAST TIDREGITAB WHERE TIDREGITAB.PERSONALKOD = PERSONALTAB.PERSONALKOD AND
      TIDREGITAB.DATUM < regdatum3 AND TIDREGITAB.TIDLOG = TRUE AND
      TIDREGITAB.OKOD1 = "" AND TIDREGITAB.PRISTYP NE "RESTID..."      
      USE-INDEX PSTART NO-LOCK NO-ERROR.
      IF NOT AVAILABLE TIDREGITAB THEN DO:
         persrec = persrec.
      END.
      ELSE DO:           
         LOOP:
         REPEAT:
            FIND FIRST AONRTAB WHERE AONRTAB.AONR = TIDREGITAB.AONR AND 
            AONRTAB.DELNR = TIDREGITAB.DELNR USE-INDEX AONR NO-LOCK NO-ERROR.  
            IF NOT AVAILABLE AONRTAB THEN DO:
               RUN nastatid_UI.
               IF AVAILABLE TIDREGITAB THEN NEXT LOOP.
               ELSE LEAVE LOOP.         
            END.
            ELSE DO:                                   
               IF AONRTAB.AONRAVDATUM = 01/01/1991 OR
               AONRTAB.AONRAVDATUM >= regdatum THEN regdatum = regdatum. 
               ELSE DO:                                                                        
                  RUN nastatid_UI.
                  IF AVAILABLE TIDREGITAB THEN NEXT LOOP.
                  ELSE LEAVE LOOP.  
               END.
            END.
	    regdatum2 = TIDREGITAB.DATUM.
	    FIND FIRST OVERAVTAB WHERE OVERAVTAB.DATUM = regdatum2 AND
	    OVERAVTAB.KOD = ANSTFORMTAB.KOD USE-INDEX ODATUM NO-LOCK NO-ERROR.
	    IF NOT AVAILABLE OVERAVTAB THEN DO:
	       LEAVE LOOP.
	    END.
	    ELSE DO:
	       IF OVERAVTAB.EQDAG = 7 OR OVERAVTAB.EQDAG = 1 
	       OR OVERAVTAB.DAGEQ = "KLA" THEN DO:
	          RUN nastatid_UI.
                  IF AVAILABLE TIDREGITAB THEN NEXT LOOP.
                  ELSE LEAVE LOOP.  
	       END.   
	       ELSE LEAVE LOOP.
	    END.
	 END. 
	 IF NOT AVAILABLE TIDREGITAB THEN NEXT START.
	 DO TRANSACTION:
	    regdatum = idag. /*ON™DIGT*/
	    CREATE tidbuff.
	    ASSIGN 
	    tidbuff.PERSONALKOD = TIDREGITAB.PERSONALKOD 
	    tidbuff.DATUM = regdatum
	    tidbuff.PROGRAM = 'AUREG' + STRING(TODAY) + globanv
	    tidbuff.VECKONUMMER = regvnr 
	    tidbuff.DAG = regdagnamn
	    tidbuff.START = regstart 
	    tidbuff.SLUT = regslut
	    tidbuff.AONR = TIDREGITAB.AONR  
	    tidbuff.DELNR = TIDREGITAB.DELNR 
	    tidbuff.TRAKTAMENTE = TIDREGITAB.TRAKTAMENTE 
	    tidbuff.OVERTIDUTTAG = TIDREGITAB.OVERTIDUTTAG 
	    tidbuff.UTRYCKNING = TIDREGITAB.UTRYCKNING
	    tidbuff.PRISTYP = TIDREGITAB.PRISTYP 
	    tidbuff.PRIS = TIDREGITAB.PRIS 
	    tidbuff.TIDLOG = TRUE. 
	    ASSIGN
	    tidtabrec = RECID(tidbuff)
	    plustid = 00.00
	    plusaonr = tidbuff.AONR
	    plusdnr = tidbuff.DELNR
	    nytid = regstart.
	    RUN TIMSEK.P.          
	    ASSIGN
	    regstartsek = sekunder
	    nytid = regslut.
	    RUN TIMSEK.P.         
	    ASSIGN
	    regslutsek = sekunder
	    plustidrec = RECID(tidbuff).
            RUN TOTTID.P.
	    ASSIGN tidbuff.TOTALT = nytid.	   
	    RUN TIDUPPDE.P.
         END. 
      END.
   END. 
   ELSE DO: 
      IF (globforetag = "GRAN" OR globforetag = "GADM" OR 
      globforetag = "ROSL" OR globforetag = "MALA" OR globforetag = 'GSYD'
      OR globforetag = "ELPA" OR globforetag = "NORD") THEN DO:     
         tidtabrec = RECID(TIDREGITAB).
         IF TIDREGITAB.START <= regstart + 1.00 THEN DO TRANSACTION:  
            /*FYLL UT MED DAGENS TIDREG*/
            RUN utfyllback_UI.            
         END.
         ELSE DO: 
            /*HITTA F™REGENDE*/           
            justtid = TIDREGITAB.START.
            RUN nastatid_UI.   
            IF NOT AVAILABLE TIDREGITAB THEN DO:
               RUN utfyllback_UI.  
               NEXT START.          
            END.
            IF TIDREGITAB.PRISTYP = "FRNVARO." THEN DO:
               RUN utfyllback_UI.  
               NEXT START.       
            END.
            LOOPGRAN:
	    REPEAT:
	       FIND FIRST AONRTAB WHERE AONRTAB.AONR = TIDREGITAB.AONR AND 
               AONRTAB.DELNR = TIDREGITAB.DELNR USE-INDEX AONR NO-LOCK NO-ERROR.  
               IF NOT AVAILABLE AONRTAB THEN DO:
                  RUN nastatid_UI.
                  IF AVAILABLE TIDREGITAB THEN NEXT LOOPGRAN.
                  ELSE LEAVE LOOPGRAN.         
               END.
               ELSE DO:                                              
                  IF AONRTAB.AONRAVDATUM = 01/01/1991 OR
                  AONRTAB.AONRAVDATUM >= regdatum THEN regdatum = regdatum. 
                  ELSE DO:                                                                         
                     RUN nastatid_UI.
                     IF AVAILABLE TIDREGITAB THEN NEXT LOOPGRAN.
                     ELSE LEAVE LOOPGRAN.  
                  END.
               END.
	       regdatum2 = TIDREGITAB.DATUM.
	       FIND FIRST OVERAVTAB WHERE OVERAVTAB.DATUM = regdatum2 AND
	       OVERAVTAB.KOD = ANSTFORMTAB.KOD USE-INDEX ODATUM NO-LOCK NO-ERROR.
	       IF NOT AVAILABLE OVERAVTAB THEN DO:
	          LEAVE LOOPGRAN.
	       END.
	       ELSE DO:
	          IF OVERAVTAB.EQDAG = 7 OR OVERAVTAB.EQDAG = 1
	          OR OVERAVTAB.DAGEQ = "KLA" THEN DO:
	             RUN nastatid_UI.
                     IF AVAILABLE TIDREGITAB THEN NEXT LOOPGRAN.
                     ELSE LEAVE LOOPGRAN.  
	          END.   
	          ELSE LEAVE LOOPGRAN.
	       END.
	    END.
	    IF NOT AVAILABLE TIDREGITAB THEN NEXT START.
	    DO TRANSACTION:
	       regdatum = idag. /*ON™DIGT*/
	       CREATE tidbuff.
	       ASSIGN 
	       tidbuff.PERSONALKOD = TIDREGITAB.PERSONALKOD 
	       tidbuff.DATUM = regdatum
	       tidbuff.PROGRAM = 'AUREG' + STRING(TODAY) + globanv
	       tidbuff.VECKONUMMER = regvnr 
	       tidbuff.DAG = regdagnamn
	       tidbuff.START = regstart 
	       tidbuff.SLUT = justtid
  	       tidbuff.AONR = TIDREGITAB.AONR  
 	       tidbuff.DELNR = TIDREGITAB.DELNR 
	       tidbuff.TRAKTAMENTE = TIDREGITAB.TRAKTAMENTE 
	       tidbuff.OVERTIDUTTAG = TIDREGITAB.OVERTIDUTTAG 
	       tidbuff.UTRYCKNING = TIDREGITAB.UTRYCKNING
	       tidbuff.PRISTYP = TIDREGITAB.PRISTYP 
	       tidbuff.PRIS = TIDREGITAB.PRIS 
	       tidbuff.TIDLOG = TRUE.    
	       ASSIGN
	       tidtabrec = RECID(tidbuff)
	       plustid = 00.00
	       plusaonr = tidbuff.AONR
	       plusdnr = tidbuff.DELNR
	       nytid = regstart.
	       RUN TIMSEK.P.          
	       ASSIGN
	       regstartsek = sekunder
	       nytid = regslut.
	       RUN TIMSEK.P.         
	       ASSIGN
	       regslutsek = sekunder
	       plustidrec = RECID(tidbuff).
               RUN TOTTID.P.
	       ASSIGN tidbuff.TOTALT = nytid.	     
	       RUN TIDUPPDE.P.
            END.  
         END.
      END.
   END.
END.
OUTPUT TO /eko1/guru/nattkoll  APPEND.
PUT "TIDER KLARA0 " TODAY " " STRING(TIME,"HH:MM:SS") SKIP.
OUTPUT CLOSE.           
FOR EACH TIDREGITAB WHERE TIDREGITAB.TOTALT = ?:
   IF TIDREGITAB.GODKAND = "" THEN DELETE TIDREGITAB.
   ELSE DO:
      OUTPUT TO autofel APPEND.
      PUT UNFORMATTED TODAY
      TIDREGITAB.PERSONALKOD TIDREGITAB.DATUM TIDREGITAB.VECKONUMMER 
      TIDREGITAB.START TIDREGITAB.SLUT TIDREGITAB.TOTALT 
      TIDREGITAB.AONR TIDREGITAB.DELNR TIDREGITAB.PROGRAM.
   END.  
   OUTPUT CLOSE.
END.   
OUTPUT TO /eko1/guru/nattkoll  APPEND.
PUT "TIDER KLARA1 " TODAY " " STRING(TIME,"HH:MM:SS") SKIP.
OUTPUT CLOSE.      
IF globforetag = "NORD" THEN DO:
   OUTPUT TO /eko1/guru/nattkoll  APPEND.
   PUT "TIDER KLARA " TODAY " " STRING(TIME,"HH:MM:SS") SKIP.
   OUTPUT CLOSE.
   
   OUTPUT TO /eko1/guru/nattkoll  APPEND.
   PUT "VECKO OCH AONR TILL EKONOMI KLARA " TODAY " " STRING(TIME,"HH:MM:SS") SKIP.
   OUTPUT CLOSE.
                          
   OUTPUT TO /eko1/guru/nattkoll  APPEND.
   PUT "KOSTREG IN FRAN EKONOMI KLARA " TODAY " " STRING(TIME,"HH:MM:SS") SKIP.
   OUTPUT CLOSE.
                          
   OUTPUT TO /eko1/guru/nattkoll  APPEND.
   PUT "AR UPP KLARA " TODAY " " STRING(TIME,"HH:MM:SS") SKIP.
   OUTPUT CLOSE.
                          
   OUTPUT TO /eko1/guru/nattkoll  APPEND.
   PUT "DAG UPP KLARA " TODAY " " STRING(TIME,"HH:MM:SS") SKIP.
   OUTPUT CLOSE.     
   /*RUN FAKMED.P*/
END.
IF (globforetag = "GRAN" OR globforetag = "GADM" OR 
globforetag = "ROSL" OR globforetag = "MALA" OR globforetag = 'GSYD') THEN DO:        
   RUN SUMAR.P.  
   RUN SUMDAG.P.     
   IF (globforetag = "GRAN" OR globforetag = 'GSYD') THEN RUN SUCCMED.P.
   IF CONNECTED("dv") THEN DO:
      musz = musz.
   END.
   ELSE DO: 
      CONNECT -db dv -S dvserver -H elvin.forse -N tcp NO-ERROR.       
   END.
   IF CONNECTED("dv") THEN DO:
      RUN AUREGGR.P.  /*F™R TALSVAR*/
      DISCONNECT dv NO-ERROR. 
   END.           
   /*IF globforetag = "GRAN" THEN RUN EL9AONR.P.*/
END.   
   
/* **********************  Internal Procedures  *********************** */


PROCEDURE nastatid_UI :
   FIND PREV TIDREGITAB WHERE
   TIDREGITAB.PERSONALKOD = PERSONALTAB.PERSONALKOD AND
   TIDREGITAB.DATUM < regdatum3 AND TIDREGITAB.TIDLOG = TRUE AND
   TIDREGITAB.OKOD1 = "" AND TIDREGITAB.PRISTYP NE "RESTID..." 
   /*TIDREGITAB.START GE regstart AND TIDREGITAB.SLUT LE regslut */
   USE-INDEX PSTART NO-LOCK NO-ERROR.
END PROCEDURE.     
PROCEDURE utfyllback_UI:
   FIND TIDREGITAB WHERE RECID(TIDREGITAB) = tidtabrec EXCLUSIVE-LOCK NO-ERROR.
   ASSIGN TIDREGITAB.START = regstart
   TIDREGITAB.PROGRAM = 'AUREGB' + STRING(TODAY) + globanv.
   nytid = TIDREGITAB.TOTALT.
   RUN TIMSEK.P.
   ASSIGN
   plustid = sekunder
   plusaonr = TIDREGITAB.AONR
   plusdnr = TIDREGITAB.DELNR
   plustidrec = RECID(TIDREGITAB)
   nytid = TIDREGITAB.START.
   RUN TIMSEK.P.
   regstartsek = sekunder.
   nytid = TIDREGITAB.SLUT.
   RUN TIMSEK.P.
   regslutsek = sekunder.
   regdatum = TIDREGITAB.DATUM.
   RUN TOTTID.P.
   ASSIGN TIDREGITAB.TOTALT = nytid.                          
   IF (globforetag = "GRAN" OR globforetag = "ELPA") THEN DO:     
      IF PERSONALTAB.LAGBAS = TRUE THEN RUN LAGBAS.P.                                 
   END. 
END PROCEDURE.
