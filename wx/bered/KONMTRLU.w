                        GET NEXT mtrlq.                     
                     END.   
                     CLOSE QUERY mtrlq.
                  END.
               END.
               antal_raknare = antal_raknare + 1.   
            END.
            
            FOR EACH emtrlbertemp:
               FIND FIRST mtrlbertemp WHERE mtrlbertemp.KTYPKOD = emtrlbertemp.KTYPKOD AND
