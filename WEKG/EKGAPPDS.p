PROCEDURE LaddaJmfKatalog :
   DEFINE INPUT PARAMETER listaP AS CHARACTER NO-UNDO. 
   DEFINE INPUT PARAMETER ekgsubidvar1 AS INTEGER.
   DEFINE INPUT PARAMETER ekgsubidvar2 AS INTEGER.
   DEFINE INPUT-OUTPUT PARAMETER TABLE-HANDLE ekgArbkodJmfTTouth .
   DEFINE INPUT-OUTPUT PARAMETER TABLE-HANDLE ekgLopnrJmfTTouth .
   DEFINE OUTPUT PARAMETER TABLE FOR ekgarbkodextraanmTT.
   
   DEFINE VARIABLE orgtabarbch AS CHARACTER NO-UNDO.
   DEFINE VARIABLE orgtablopbch AS CHARACTER NO-UNDO.
   DEFINE VARIABLE orgbufarbh AS HANDLE NO-UNDO.
   DEFINE VARIABLE orgbufloph AS HANDLE NO-UNDO.
   DEFINE VARIABLE queh AS HANDLE NO-UNDO.
      
   DEFINE VARIABLE ekgArbkodJmfTTh AS HANDLE NO-UNDO.
   DEFINE VARIABLE ekgLopnrJmfTTh AS HANDLE NO-UNDO.
   
   EMPTY TEMP-TABLE ekgarbkodextraanmTT NO-ERROR.
   
   IF VALID-HANDLE(ekgArbkodJmfTTouth) THEN ekgArbkodJmfTTh = ekgArbkodJmfTTouth:DEFAULT-BUFFER-HANDLE.
   ekgLopnrJmfTTh = ekgLopnrJmfTTouth:DEFAULT-BUFFER-HANDLE.
   
   ekgArbkodJmfTTh:EMPTY-TEMP-TABLE() NO-ERROR.
   ekgLopnrJmfTTh:EMPTY-TEMP-TABLE() NO-ERROR.
   
   FOR EACH EKGARBKODEXTRAANM WHERE EKGARBKODEXTRAANM.EKGSUBID = ekgsubidvar1 NO-LOCK:
      CREATE ekgarbkodextraanmTT.
      BUFFER-COPY EKGARBKODEXTRAANM TO ekgarbkodextraanmTT.
   END.
   
   IF listaP = "P1" THEN DO:
      orgtabarbch = "EKGP1ARBKOD".
      orgtablopbch = "EKGP1".
   END.
   IF listaP = "P2" THEN DO:
      orgtabarbch = "EKGP2ARBKOD".
      orgtablopbch = "EKGP2".
   END.
   
   IF listaP = "P3" THEN DO:
      orgtabarbch = "EKGP3ARBKOD".
      orgtablopbch = "EKGP3".
   END.
   IF listaP = "M" THEN DO:
      orgtabarbch = "".
      orgtablopbch = "EKGMTRL".
   END.
  
   
   IF orgtabarbch NE "" THEN DO:
      CREATE BUFFER orgbufarbh FOR TABLE orgtabarbch.
      CREATE QUERY queh.
      queh:SET-BUFFERS(orgbufarbh).
      queh:QUERY-PREPARE("FOR EACH " + orgbufarbh:TABLE + " WHERE EKGSUBID = " + STRING(ekgsubidvar1) + " NO-LOCK.").
      queh:QUERY-OPEN().
      queh:GET-FIRST(NO-LOCK).
      DO WHILE queh:QUERY-OFF-END = FALSE:
         /* Kopierar all data från skarp- till temp-tabell*/
         ekgArbkodJmfTTh:BUFFER-CREATE().
         ekgArbkodJmfTTh:BUFFER-COPY(orgbufarbh).
         queh:GET-NEXT(NO-LOCK).
      END.
      queh:QUERY-CLOSE.
   END.
   CREATE BUFFER orgbufloph FOR TABLE orgtablopbch.
   CREATE QUERY queh.
   queh:SET-BUFFERS(orgbufloph).
   queh:QUERY-PREPARE("FOR EACH " + orgbufloph:TABLE + " WHERE EKGSUBID = " + STRING(ekgsubidvar1) + " NO-LOCK.").
   queh:QUERY-OPEN().
   queh:GET-FIRST(NO-LOCK).
   DO WHILE queh:QUERY-OFF-END = FALSE:
      /* Kopierar all data från skarp- till temp-tabell*/
      ekgLopnrJmfTTh:BUFFER-CREATE().
      ekgLopnrJmfTTh:BUFFER-COPY(orgbufloph).
      queh:GET-NEXT(NO-LOCK).
   END.
   queh:QUERY-CLOSE.
   CREATE QUERY queh.
   queh:SET-BUFFERS(orgbufloph).
   queh:QUERY-PREPARE("FOR EACH " + orgbufloph:TABLE + " WHERE EKGSUBID = " + STRING(ekgsubidvar2) + " NO-LOCK.").
   queh:QUERY-OPEN().
   queh:GET-FIRST(NO-LOCK).
   DO WHILE queh:QUERY-OFF-END = FALSE:
      /* Kopierar all data från skarp- till temp-tabell*/
      ekgLopnrJmfTTh:BUFFER-CREATE().
      ekgLopnrJmfTTh:BUFFER-COPY(orgbufloph).
      queh:GET-NEXT(NO-LOCK).
   END.
   queh:QUERY-CLOSE.
   DELETE OBJECT queh NO-ERROR.
   queh = ?.
   DELETE OBJECT orgbufarbh NO-ERROR.
   orgbufarbh = ?.
   DELETE OBJECT orgbufloph NO-ERROR.
   orgbufloph = ?.
   /*
   DELETE OBJECT ekgArbkodJmfTTouth NO-ERROR.
   ekgArbkodJmfTTouth = ?.
   DELETE OBJECT ekgLopnrJmfTTouth NO-ERROR.
   ekgLopnrJmfTTouth = ?.
   */
END PROCEDURE.

