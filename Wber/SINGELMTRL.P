OS-APPEND "C:\TEMP\elektroskandia.d"  "C:\TEMP\onelah.d".
OS-APPEND "C:\TEMP\ahlsell.d"  "C:\TEMP\onelah.d".
OS-APPEND "C:\TEMP\onninen.d"  "C:\TEMP\onelah.d".
*/