      iColumn = iColumn + 1 .
      cColumn = STRING(iColumn).
      cRange = "A" + cColumn.
      hjtext = valort.
      chWorkSheet:Range(cRange):Font:Bold = TRUE NO-ERROR.     
      RUN utexcel_UI (INPUT "ARIAL",INPUT 10,INPUT hjtext).
      iColumn = iColumn + 1 .
      cColumn = STRING(iColumn).
      cRange = "A" + cColumn.
      chWorkSheet:Range(cRange):Font:Bold = TRUE NO-ERROR.
      hjtext = "Beredning nr: " + STRING(valaonr).     
      RUN utexcel_UI (INPUT "ARIAL",INPUT 10,INPUT hjtext).
