/*PERSEPOST.P */
DEFINE TEMP-TABLE tempepost NO-UNDO
   FIELD NAMN AS CHARACTER
   FIELD EPOST AS CHARACTER
   INDEX NAMN NAMN
   INDEX EPOST EPOST.
DEFINE OUTPUT PARAMETER TABLE FOR tempepost.
   FOR EACH PERSONALTAB NO-LOCK:
      CREATE tempepost.
      ASSIGN 
      tempepost.NAMN =  PERSONALTAB.FORNAMN + " " + PERSONALTAB.EFTERNAMN
      tempepost.EPOST = SUBSTRING(PERSONALTAB.PERSONSOK,20).
      IF SUBSTRING(PERSONALTAB.PERSONSOK,20) = "" THEN tempepost.EPOST = "Epost saknas". 
      Guru.GlobalaVariabler:GDPRvem = Guru.GlobalaVariabler:GDPRvem + "," + PERSONALTAB.PERSONALKOD.

   END.
