   FOR EACH  KFORMAN WHERE KFORMAN.TRAAVTAL = FILL-IN-FRANBYT:
      ASSIGN                
      KFORMAN.TRAAVTAL = FILL-IN-NYTTAVBYT. 
   END.   
END.
IF TOG_LONFLER = TRUE THEN DO TRANSACTION:
   FOR EACH LONFLER WHERE LONFLER.TRAAVTAL = FILL-IN-FRANBYT:    
      ASSIGN                   
      LONFLER.TRAAVTAL = FILL-IN-NYTTAVBYT.
   END.   
END.
IF TOG_MALTAB = TRUE THEN DO TRANSACTION: 
   FOR EACH MALTAB WHERE MALTAB.TRAAVTAL = FILL-IN-FRANBYT:     
      ASSIGN         
      MALTAB.TRAAVTAL = FILL-IN-NYTTAVBYT.
   END.   
END.
IF TOG_TRAKTASTART = TRUE THEN DO TRANSACTION:
   FOR EACH TRAKTASTART WHERE TRAKTASTART.TRAAVTAL = FILL-IN-FRANBYT:      
      ASSIGN    
      TRAKTASTART.TRAAVTAL = FILL-IN-NYTTAVBYT.
   END.   
END.
IF TOG_TRAKTATAB = TRUE THEN DO TRANSACTION:
   FOR EACH TRAKTATAB WHERE TRAKTATAB.TRAAVTAL = FILL-IN-FRANBYT:   
      ASSIGN    
      TRAKTATAB.TRAAVTAL = FILL-IN-NYTTAVBYT.
   END.   
END.
IF TOG_TRAKTFLER = TRUE THEN DO TRANSACTION:
   FOR EACH TRAKTFLER WHERE TRAKTFLER.TRAAVTAL = FILL-IN-FRANBYT:   
      ASSIGN    
      TRAKTFLER.TRAAVTAL = FILL-IN-NYTTAVBYT.
   END.   
END.
IF TOG_TRAKTREGLER = TRUE THEN DO TRANSACTION:
