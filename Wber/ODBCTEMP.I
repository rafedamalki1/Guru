/*ODBCTEMP.I*/
DEFINE TEMP-TABLE sqltab NO-UNDO
   FIELD TABNAMN  AS CHARACTER FORMAT "x(30)"
   FIELD FALTANT  AS INTEGER
   FIELD FALT AS CHARACTER EXTENT 50
   FIELD FALTTYP AS CHARACTER FORMAT "x(30)" EXTENT 50
   INDEX TABNAMN TABNAMN.
DEFINE TEMP-TABLE sqldat NO-UNDO 
   FIELD TABNAMN  AS CHARACTER
   FIELD DATAFALT AS CHARACTER EXTENT 50
   INDEX TABNAMN TABNAMN.
DEFINE TEMP-TABLE Esqldat NO-UNDO LIKE sqldat. 
DEFINE TEMP-TABLE vadgora NO-UNDO 
   FIELD VAD AS LOGICAL EXTENT 10.
   
