       ELSE DO:
          cRangefont = "A" + cColumn + ":" + "P" + cColumn.
          IF iColumn = 3 THEN DO:
             chWorkSheet:Range(cRangefont):Borders(4):Weight = 2 NO-ERROR. /*3= över 4= under, 2= tunn 4= tjock*/
          END.
          ELSE DO:
             chWorkSheet:Range(cRangefont):Borders(3):Weight = 2 NO-ERROR. /*3= över 4= under, 2= tunn 4= tjock*/
