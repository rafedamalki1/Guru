/*delalldep.p*/
   
   OPEN QUERY mtrlq FOR EACH MTRLDEP  NO-LOCK.
   GET FIRST mtrlq NO-LOCK.
   DO WHILE AVAILABLE(MTRLDEP):               
      DO TRANSACTION:
         GET CURRENT mtrlq EXCLUSIVE-LOCK.
         DELETE MTRLDEP.
      END.                  
      GET NEXT mtrlq NO-LOCK.
   END.    
   CLOSE QUERY mtrlq.
   OPEN QUERY bestq FOR EACH BESTDEP  NO-LOCK.
   GET FIRST bestq NO-LOCK.
   DO WHILE AVAILABLE(BESTDEP):           
      DO TRANSACTION:
         GET CURRENT bestq EXCLUSIVE-LOCK.
         DELETE BESTDEP.
      END.
      GET NEXT bestq NO-LOCK.               
   END.
