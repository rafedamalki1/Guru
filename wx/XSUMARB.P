         nattpris = TIDREGITAB.PRIS	      ovkod = TIDREGITAB.OKOD1
         nytid = TIDREGITAB.OANT1.
         RUN TIMSEK.P.
         ovantal = (sekunder / 3600).
         RUN over_UI.		      
         RUN skapa_UI.   
