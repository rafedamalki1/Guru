               IF mkskrub = TRUE THEN DO: 
                  FIND FIRST ekon_val WHERE ekon_val.NUM = kon_val.NUM and ekon_val.KSKAP = FALSE NO-ERROR.
                  IF AVAILABLE ekon_val THEN DO:
                     RUN elskylt_UI.
                     FOR EACH ekon_val WHERE ekon_val.NUM = kon_val.NUM :
                        DELETE ekon_val. 
