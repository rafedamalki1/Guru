   chWorkSheet:Range("E1:E300"):NumberFormat = "@".   
   ASSIGN   
   numskylt = 0.
   IF mall = 1 THEN DO:
      IF skyltvar = 2 THEN.
      ELSE DO:         
         FOR EACH kon_val WHERE kon_val.KSKAP = FALSE USE-INDEX ORD:      
            FIND FIRST kon_id WHERE kon_id.NUM = kon_val.NUM NO-LOCK NO-ERROR.         
            IF AVAILABLE kon_id THEN DO:
               {EXCELFEL.I}     
