                     GET NEXT kq NO-LOCK.
                  END.
                  CLOSE QUERY kq.
                  RUN storning3_UI.               
               END.            
               RUN slutomr_UI.
            END.
            RUN str_UI.
            RUN slutallt_UI.
            RUN str2_UI.
         END.
         ELSE DO:
            FIND FIRST stordriftomrtemp WHERE stordriftomrtemp.STDRIFTID = 2 NO-LOCK NO-ERROR.
            CREATE tidut.
            ASSIGN
            SUBSTRING(tidut.UT,1) = stordriftomrtemp.NAMN
