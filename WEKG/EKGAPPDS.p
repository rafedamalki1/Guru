PROCEDURE PSP2K_UI :
   DEFINE VARIABLE lastid     AS INTEGER NO-UNDO.
   DEFINE VARIABLE ORDNINGVAR AS INTEGER NO-UNDO.
   INPUT FROM VALUE("C:\protemp11\elpao\vp2kom2014.skv") NO-ECHO.
   REPEAT:
      
      DO TRANSACTION: 
         CREATE tidin.
         ASSIGN.
         IMPORT DELIMITER ";" tidin   NO-ERROR.            
      END. 
   END.    
   INPUT CLOSE.
   FOR EACH tidin:
      IF tidin.BENAMNING = "" THEN  DELETE tidin.
      ELSE DO:
         FIND FIRST EKGP2KOM  WHERE EKGP2KOM.EKGSUBID   = 12 AND EKGP2KOM.ANMARKNING = tidin.BENAMNING NO-LOCK NO-ERROR.
         IF NOT AVAILABLE EKGP2KOM THEN DO:
            FIND LAST  EKGP2KOM  WHERE EKGP2KOM.EKGSUBID   = 12 NO-LOCK NO-ERROR.
            IF AVAILABLE EKGP2KOM THEN lastid = EKGP2KOM.KOMID + 1.
            ELSE lastid = 1.
            CREATE EKGP2KOM.
            ASSIGN
               EKGP2KOM.EKGSUBID   = 12
               EKGP2KOM.EBRKAT     = "KLG2"
               EKGP2KOM.KOMID      = lastid  
               EKGP2KOM.ANMARKNING = tidin.BENAMNING.   
         END.

      
      END.
   END.
   FOR EACH tidin:
      FIND FIRST EKGP2KOM  WHERE EKGP2KOM.EKGSUBID   = 12 AND EKGP2KOM.ANMARKNING = tidin.BENAMNING NO-LOCK NO-ERROR.
      IF AVAILABLE EKGP2KOM THEN DO:
         FIND LAST EKGP2ARBSVEPKOM  WHERE EKGP2ARBSVEPKOM.EKGSUBID   = 12 AND EKGP2ARBSVEPKOM.P2ARBKOD =  TRIM(tidin.ARBKOD) AND  EKGP2ARBSVEPKOM.SVEPLOPNR =  TRIM(tidin.LOPNR) 
         NO-LOCK NO-ERROR.
         IF AVAILABLE EKGP2ARBSVEPKOM THEN ORDNINGVAR = EKGP2ARBSVEPKOM.ORDNING + 1.
         ELSE ORDNINGVAR = 1.
         CREATE EKGP2ARBSVEPKOM.
         BUFFER-COPY EKGP2KOM TO EKGP2ARBSVEPKOM.
         ASSIGN
         EKGP2ARBSVEPKOM.P2ARBKOD =  TRIM(tidin.ARBKOD)
         EKGP2ARBSVEPKOM.SVEPLOPNR = TRIM(tidin.LOPNR)
         EKGP2ARBSVEPKOM.ORDNING   = ORDNINGVAR  
         EKGP2ARBSVEPKOM.INGAREJ   = tidin.INGAREJ.   
         EKGP2ARBSVEPKOM.SVEPLOPNR = LEFT-TRIM(EKGP2ARBSVEPKOM.SVEPLOPNR,".").
      END.
   END.
   /*
   OUTPUT TO "c:\protemp11\komkoll.txt".
   FOR EACH EKGP2KOM  WHERE EKGP2KOM.EKGSUBID   = 12 NO-LOCK BY EKGP2KOM.komid : 
      FOR EACH EKGP2ARBSVEPKOM WHERE EKGP2ARBSVEPKOM.EKGSUBID   = 12 AND EKGP2ARBSVEPKOM.komid = EKGP2KOM.komid NO-LOCK BY EKGP2ARBSVEPKOM.P2ARBKOD BY EKGP2ARBSVEPKOM.SVEPLOPNR :
         PUT unformatted EKGP2ARBSVEPKOM.komid AT 1  " " EKGP2ARBSVEPKOM.P2ARBKOD " " EKGP2ARBSVEPKOM.SVEPLOPNR " " EKGP2ARBSVEPKOM.ANMARKNING SKIP.
       END.   
   END.
   OUTPUT CLOSE.

   */
END PROCEDURE.

