      /*EJ CLASSER*/
      IF varforetypval[36] = 1 THEN DO:
         
         /*endast p2*/
         
            FIND FIRST LOP2 WHERE LOP2.KATAR = hjkatvar[1] AND LOP2.ARBKOD = KALKBER.ARBKOD AND LOP2.LOPNR = KALKBER.LOPNR NO-LOCK NO-ERROR.
          
        
