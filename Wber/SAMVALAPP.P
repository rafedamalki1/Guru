{STARTFORAPP.I}
{SCHAKTADM.I}

PROCEDURE samvalhmt_UI :
   DEFINE OUTPUT PARAMETER TABLE FOR samforlaggtemp.
   EMPTY TEMP-TABLE samforlaggtemp NO-ERROR. 
   FOR EACH SAMFORLAGG NO-LOCK:
      CREATE samforlaggtemp.
      BUFFER-COPY SAMFORLAGG TO samforlaggtemp.
      ASSIGN samforlaggtemp.SAMFORROW = ROWID(SAMFORLAGG).
   END.
   RETURN.
END PROCEDURE.
