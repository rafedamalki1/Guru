   IF Guru.Konstanter:appcon = TRUE THEN DO:
      RUN LaddakonstanterApp.P ON Guru.Konstanter:apphand TRANSACTION DISTINCT. 
   END.  
   
   RUN sek_UI.
   IF Guru.Konstanter:appcon = TRUE THEN DO:
      RUN sek_UI IN losenwebh. 
