   IF skrivbord = FALSE THEN DO:
      FOR EACH id_temp USE-INDEX ORD NO-LOCK:
          FOR EACH uppl_temp WHERE uppl_temp.UPPLAG NE ? AND uppl_temp.NUM = id_temp.NUM NO-LOCK:
