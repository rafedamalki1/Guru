            IF pansv = "" OR pansv = "ALLA" THEN DO:
               IF PLANNRTAB.OMRADE = omrkonto.OMRADE THEN DO:
                  RUN plannr_UI.
               END.
            END.
            ELSE DO:
               IF PLANNRTAB.OMRADE = omrkonto.OMRADE AND PLANNRTAB.ARBANSVARIG = pansv THEN DO:
                  
                  RUN plannr_UI.
               END.
            END. 
         END.
         GET NEXT kq NO-LOCK.
      END.   
      CLOSE QUERY kq.
      RUN slutkonto_UI.
      CREATE tidut.
   END.   
   RUN slutomrade_UI.
   CREATE tidut.
END.
