    method public logical RemoveRibbonGroup 
          (ioGroup as Progress.Lang.Object):
                    
        DEFINE VARIABLE ix           AS INT NO-UNDO.   
        DEFINE VARIABLE iCnt         AS INT NO-UNDO.           
        DEFINE VARIABLE ribbonGroup1 AS Infragistics.Win.UltraWinToolbars.RibbonGroup NO-UNDO.
        ribbonGroup1 = cast(ioGroup,"Infragistics.Win.UltraWinToolbars.RibbonGroup").
        iCnt = ribbonGroup1:Tools:Count.
        
        DO ix = 1 to iCnt:
          if ribbonGroup1:Tools:Item[0]:GetClass():TypeName = "Infragistics.Win.UltraWinToolbars.ButtonTool" then
            this-object:RemoveRibbonButtonTool(cast(ribbonGroup1:Tools:Item[0],"Progress.Lang.Object")).
          else if ribbonGroup1:Tools:Item[0]:GetClass():TypeName = "Infragistics.Win.UltraWinToolbars.StateButtonTool" then
            this-object:RemoveRibbonStateButtonTool(cast(ribbonGroup1:Tools:Item[0],"Progress.Lang.Object")).
          else if ribbonGroup1:Tools:Item[0]:GetClass():TypeName = "Infragistics.Win.UltraWinToolbars.PopupMenuTool" then
            this-object:RemoveRibbonPopupMenuTool(cast(ribbonGroup1:Tools:Item[0],"Progress.Lang.Object")).
        END.      
        
        ribbonGroup1:Tab:Groups:Remove(ribbonGroup1).
        ribbonGroup1:Dispose().
        return yes. 
    end method.    
    
