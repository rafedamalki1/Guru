            RUN str_UI.
            RUN slutomr_UI.
            CREATE tidut.      
            FOR EACH stordriftomrtemp WHERE stordriftomrtemp.STDRIFTID NE 2 USE-INDEX STDRIFTID NO-LOCK:                  
               CREATE tidut. 
