/*XGEOMR.P*/
OPEN QUERY SQ FOR EACH SUMTID WHERE SUMTID.GEOMRADE = "" NO-LOCK.
GET FIRST SQ NO-LOCK.
DO WHILE AVAILABLE(SUMTID):
   DO TRANSACTION:
      GET CURRENT SQ EXCLUSIVE-LOCK.
      SUMTID.GEOMRADE = SUMTID.OMRADE.
      
   END.
   GET NEXT SQ NO-LOCK.
END.
OPEN QUERY SQ2 FOR EACH SUMTIDDAG WHERE SUMTIDDAG.GEOMRADE = "" NO-LOCK.
GET FIRST SQ2 NO-LOCK.
DO WHILE AVAILABLE(SUMTIDDAG):
   DO TRANSACTION:
      GET CURRENT SQ2 EXCLUSIVE-LOCK.
      SUMTIDDAG.GEOMRADE = SUMTIDDAG.OMRADE.      
   END.
   GET NEXT SQ2 NO-LOCK.
END.
