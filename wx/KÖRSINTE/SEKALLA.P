PROCEDURE nextguru_UI:
   FIND FIRST xsektemp WHERE xsektemp.MENYVART = xhop AND
   xsektemp.AV-LEVEL = Guru.Konstanter:globniv USE-INDEX XSEK NO-LOCK NO-ERROR.  
END PROCEDURE.
IF xhop = "GURU" OR xhop = "ALLA" THEN DO:
   RUN nextguru_UI.
