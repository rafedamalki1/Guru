PROCEDURE LaddaP2kom :
   DEFINE INPUT PARAMETER ekgsubidvar AS INTEGER.
   DEFINE INPUT PARAMETER ebrkatvar AS CHARACTER.
   DEFINE OUTPUT PARAMETER DATASET FOR EKGP2komDS.
   DEFINE VARIABLE queryprep AS CHARACTER NO-UNDO.
    DEFINE VARIABLE queryprepsvep AS CHARACTER NO-UNDO.
   DATASET EKGP2komDS:EMPTY-DATASET().
   /*RUN p2sp.*/
   queryprep = "FOR EACH EKGP2KOM WHERE EKGP2KOM.EKGSUBID = " + STRING(ekgsubidvar) +  " NO-LOCK".
   queryprepsvep = "FOR EACH EKGP2ARBSVEPKOM WHERE EKGP2ARBSVEPKOM.EKGSUBID = " + STRING(ekgsubidvar) +  " NO-LOCK".
   QUERY EKGP2komQuery:QUERY-PREPARE(queryprep).
   QUERY EKGP2svepQuery:QUERY-PREPARE(queryprepsvep).
   RUN attachEKGP2komDS.
   DATASET EKGP2komDS:FILL().
   detachDataSetEKGP2komDS(hEKGP2komDS).
   
END PROCEDURE.
