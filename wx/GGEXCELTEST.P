PROCEDURE start_UI:
   /*Vilka kolumner*/     
   ASSIGN
   slutbredd = 10
   bredd[1] = 50
   bredd[2] = 6
   bredd[3] = 6
   bredd[4] = 6
   bredd[5] = 6
   bredd[6] = 6
   bredd[7] = 6
   bredd[8] = 6
   bredd[9] = 6
   bredd[10] = 6
   bredd[11] = 6
   bredd[12] = 6
   bredd[13] = 6
   bredd[14] = 6
   bredd[15] = 6.
   
   RUN colbredd_UI.  
   RUN startexcel_UI.
   RUN namnbladexcel_UI (INPUT "Tidsplan").
   /*Rubriker*/
   FIND FIRST tidut NO-LOCK NO-ERROR.
   raknare = 1.
   /*Kolumnbredd*/
   RUN kolumnexcel_UI.

   /*Poster*/ 
   raknare = 1.
   REPEAT:
      IF SUBSTRING(tidut.UT,1,5) = "=====" THEN DO:
         RUN understryk_UI (INPUT 4,INPUT 2).                
      END.
      ELSE DO:
         DEBUGGER:SET-BREAK().
         RUN posterexcel_UI (INPUT tidut.UT,INPUT "COURIER",INPUT 10,INPUT FALSE,INPUT 12,INPUT 0,INPUT FALSE,INPUT FALSE,INPUT 25,INPUT 143).        
      END.   
      FIND NEXT tidut NO-LOCK NO-ERROR.
      IF NOT AVAILABLE tidut THEN DO:
         LEAVE.
      END.      
   END.   
END PROCEDURE.


