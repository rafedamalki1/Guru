/*KALKIN.I*/
DEFINE {&NEW} {&SHARED} TEMP-TABLE kalkintemp NO-UNDO
   FIELD AONR AS CHARACTER 
   FIELD DELNR AS INTEGER 
   FIELD PLANNR AS CHARACTER 
   FIELD ARTAL AS INTEGER 
   FIELD REPO AS LOGICAL
   INDEX AONR IS PRIMARY AONR DELNR
   INDEX PLANNR PLANNR
   INDEX REPO REPO.

DEFINE {&NEW} {&SHARED} TEMP-TABLE kalkinmtemp NO-UNDO LIKE kalkintemp
    FIELD BENAMNING AS CHARACTER
    FIELD AONRAVDATUM AS DATE
    FIELD PLANNRAVDATUM AS DATE.
