   FIELD PRISA         AS DECIMAL
   FIELD NAMN          AS CHARACTER LABEL "Befattning"
   FIELD STARTDATUM    AS DATE
   INDEX BEF BEFATTNING
   INDEX NAMN IS PRIMARY NAMN.
