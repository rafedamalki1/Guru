PROCEDURE FriSkapTyp_UI :
   
   EMPTY TEMP-TABLE fri_temp NO-ERROR. 
   FOR EACH FRIKORT WHERE FRIKORT.AONR = valaonr AND FRIKORT.OMRADE = valomrade AND FRIKORT.KABNR > 0 EXCLUSIVE-LOCK:
      FIND FIRST fri_temp WHERE fri_temp.NUM = FRIKORT.NUM NO-LOCK NO-ERROR.
      IF AVAILABLE fri_temp THEN DO:
         /*kontrollerar att om det finns en kabel > 0 */
        IF fri_temp.skap = "" AND FRIKORT.SKAP NE "" THEN FRIKORT.SKAP  = fri_temp.SKAP.
      END.     
      IF NOT AVAILABLE fri_temp THEN DO:         
         CREATE fri_temp.
         BUFFER-COPY FRIKORT TO fri_temp.
      END.
   END.
   
/*   FOR EACH fri_temp WHERE fri_temp.SKAP = "" :
