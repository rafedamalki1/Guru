               chWorkSheet:Range(cRange):Value = SUBSTRING(tidut2.UT,69,6) NO-ERROR.
               cRange = "I" + cColumn.
               chWorkSheet:Range(cRange):Value = SUBSTRING(tidut2.UT,75,7) NO-ERROR. 
               cRange = "J" + cColumn.
               chWorkSheet:Range(cRange):Value = SUBSTRING(tidut2.UT,82,6) NO-ERROR.   
