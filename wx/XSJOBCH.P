DEFINE BUFFER A FOR TIDREGITAB.
OUTPUT TO C:\OBSJCH.TXT.
FOR EACH TIDREGITAB WHERE TIDREGITAB.DATUM GE 01/01/2012 AND TIDREGITAB.TIDLOG = TRUE
AND TIDREGITAB.PRISTYP = "FRÅNVARO." NO-LOCK:
   FIND FIRST A WHERE A.PERSONALKOD = TIDREGITAB.PERSONALKOD AND A.DATUM = TIDREGITAB.DATUM AND A.LONTILLAGG NE "" 
    NO-LOCK NO-ERROR.
   IF AVAILABLE A THEN DO:
      DISP A.DATUM A.PERSONALKOD A.LONTILLAGG A.PRISTYP.
   END.
END.   
