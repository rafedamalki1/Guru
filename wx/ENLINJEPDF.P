   RUN set_text_UI (INPUT "Po = 0,870 kW Wo = 7621").
   RUN set_text_UI (INPUT "Mätningspunkt Skolan 50 mva 45 grader Uk = 5% Dyn Uo = 400 V").
   RUN set_text_UI (INPUT "Utnyttjningstid = 4000 timmar Spänn-faktor C = 0,75 Sammanlagf-faktor = 1,00").
   RUN set_text_UI (INPUT "Po = 0,870 kW Wo = 7621").
   RUN set_text_UI (INPUT "Mätningspunkt Skolan 50 mva 45 grader Uk = 5% Dyn Uo = 400 V").
   RUN set_text_UI (INPUT "Utnyttjningstid = 4000 timmar Spänn-faktor C = 0,75 Sammanlagf-faktor = 1,00").
   RUN set_text_UI (INPUT "Po = 0,870 kW Wo = 7621").
   RUN set_text_UI (INPUT "Mätningspunkt Skolan 50 mva 45 grader Uk = 5% Dyn Uo = 400 V").
   RUN set_text_UI (INPUT "Utnyttjningstid = 4000 timmar Spänn-faktor C = 0,75 Sammanlagf-faktor = 1,00").
   RUN set_text_UI (INPUT "Po = 0,870 kW Wo = 7621").
   RUN set_text_UI (INPUT "Mätningspunkt Skolan 50 mva 45 grader Uk = 5% Dyn Uo = 400 V").
   RUN set_text_UI (INPUT "Utnyttjningstid = 4000 timmar Spänn-faktor C = 0,75 Sammanlagf-faktor = 1,00").
   RUN set_text_UI (INPUT "Po = 0,870 kW Wo = 7621").

   RUN set_verticalline (INPUT 20,INPUT 1).
