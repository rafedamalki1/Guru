      cColumn = STRING(iColumn).
      cRange = "A" + cColumn.                       
   END.
   ELSE DO:
      hjtext = Guru.Konstanter:gaok + ": " + Guru.GlobalaVariabler:plusaonr + " " + STRING(Guru.GlobalaVariabler:plusdnr,Guru.Konstanter:varforetypchar[1]).
      chWorkSheet:Range(cRange):Font:Bold = TRUE NO-ERROR.      
