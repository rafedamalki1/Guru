IF FILL-IN-START = "BIRK" THEN runvar = "VALDBBIRK.r".
IF FILL-IN-START = "BORL" THEN runvar = "VALDBBOR.r".
IF FILL-IN-START = "ELPA" THEN runvar = "KUNDDB.r".
