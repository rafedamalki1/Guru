PROCEDURE Kompinnehall_UI :

   EMPTY TEMP-TABLE kon_display NO-ERROR.
   DEFINE BUFFER kdisp FOR BERVAL.
   DEFINE VARIABLE enval AS LOGICAL NO-UNDO.
   FOR EACH innehall BREAK BY innehall.BERNR BY innehall.OMRADE:        
      ACCUMULATE innehall.NUM (TOTAL BY innehall.BERNR BY innehall.OMRADE).       
      IF LAST-OF(innehall.OMRADE) THEN DO:
         CREATE einnehall.
         ASSIGN                        
         einnehall.BERNR = innehall.BERNR 
         einnehall.OMRADE = innehall.OMRADE. 
      END.     
   END.

   FOR EACH innehall:
      FIND FIRST BERVAL WHERE BERVAL.AONR = STRING(innehall.BERNR) AND BERVAL.OMRADE = innehall.OMRADE AND BERVAL.NUM = innehall.NUM NO-LOCK NO-ERROR.
      IF AVAILABLE BERVAL THEN DO:
         innehall.UPPLAG = BERVAL.UPPLAG.
      END.
      FIND FIRST BERID WHERE BERID.AONR = STRING(innehall.BERNR) AND BERID.OMRADE = innehall.OMRADE AND BERID.NUM = innehall.NUM NO-LOCK NO-ERROR.
      IF AVAILABLE BERID THEN DO:
         innehall.BYGGNR = STRING(BERID.FRI2).
         innehall.FRIID = BERID.FRI3.
         
      END.  
      FIND FIRST BERID2 WHERE BERID2.AONR = STRING(innehall.BERNR) AND BERID2.OMRADE = innehall.OMRADE AND BERID2.NUM = innehall.NUM NO-LOCK NO-ERROR.
      IF AVAILABLE BERID2 THEN DO:
         innehall.KARTREF = BERID2.STOLP.
      END.   
   
         
   END.   
   FOR EACH einnehall WHERE NO-LOCK:
      FOR EACH BERVAL WHERE BERVAL.AONR = STRING(einnehall.BERNR) AND BERVAL.OMRADE = einnehall.OMRADE NO-LOCK BY BERVAL.AONR BY BERVAL.OMRADE BY BERVAL.NUM BY BERVAL.SKAPNUM :
         FIND FIRST kon_display WHERE kon_display.BERAONR = STRING(einnehall.BERNR) AND 
         kon_display.OMRADE = einnehall.OMRADE AND kon_display.NUM = BERVAL.NUM NO-LOCK NO-ERROR.
         IF NOT AVAILABLE kon_display THEN DO:
            CREATE kon_display.
         END.   
         IF BERVAL.KSKAP = FALSE THEN DO:
            BUFFER-COPY BERVAL TO kon_display. 
            kon_display.BERAONR = BERVAL.AONR.     
         END.    
        
         enval = FALSE.
         IF AVAILABLE BERVAL THEN DO:
           
            IF BERVAL.KONSKOD = 0 THEN enval = TRUE.   
            ELSE IF LOOKUP(STRING(BERVAL.KONSKOD),Guru.Konstanter:varforetypchar[5]) > 0 THEN enval = TRUE.
           
         END.
         IF enval = TRUE THEN DO:
            FIND FIRST kdisp WHERE kdisp.AONR = BERVAL.AONR AND kdisp.OMRADE = BERVAL.OMRADE AND kdisp.NUM = BERVAL.NUM AND kdisp.F2 NE ""  NO-LOCK NO-ERROR.
            IF AVAILABLE kdisp THEN DO:
               IF kon_display.F1 = "" THEN kon_display.F1 = kdisp.KTYPKOD.
               IF kon_display.F2 = "" THEN kon_display.F2 = kdisp.F2.
               IF kon_display.F3 = "" THEN kon_display.F3 = kdisp.F3.
               IF kon_display.F4 = "" THEN kon_display.F4 = kdisp.F4.
               IF kon_display.F5 = "" THEN kon_display.F5 = kdisp.F5.
               IF kon_display.F6 = "" THEN kon_display.F6 = kdisp.F6.
               kon_display.KSKAP = FALSE.              
            END.
            ELSE DO:
               FOR EACH kdisp WHERE  kdisp.AONR = BERVAL.AONR AND kdisp.OMRADE = BERVAL.OMRADE AND kdisp.NUM = BERVAL.NUM  NO-LOCK:
                  IF kon_display.F1 = "" THEN kon_display.F1 = kdisp.KTYPKOD.
                  IF kon_display.F2 = "" THEN kon_display.F2 = kdisp.F2.
                  IF kon_display.F3 = "" THEN kon_display.F3 = kdisp.F3.
                  IF kon_display.F4 = "" THEN kon_display.F4 = kdisp.F4.
                  IF kon_display.F5 = "" THEN kon_display.F5 = kdisp.F5.
                  IF kon_display.F6 = "" THEN kon_display.F6 = kdisp.F6.
                  kon_display.KSKAP = FALSE.               
               END.
            END.   
         END.
         ELSE DO:     
            
            FOR EACH kdisp WHERE  kdisp.AONR = BERVAL.AONR AND kdisp.OMRADE = BERVAL.OMRADE AND kdisp.NUM = BERVAL.NUM  /*AND kdisp.KSKAP = TRUE*/ NO-LOCK:
               IF kon_display.F1 = "" THEN kon_display.F1 = kdisp.KTYPKOD.
               IF kon_display.F2 = "" THEN kon_display.F2 = kdisp.F2.
               IF kon_display.F3 = "" THEN kon_display.F3 = kdisp.F3.
               IF kon_display.F4 = "" THEN kon_display.F4 = kdisp.F4.
               IF kon_display.F5 = "" THEN kon_display.F5 = kdisp.F5.
               IF kon_display.F6 = "" THEN kon_display.F6 = kdisp.F6.                         
            END.
         END.
         enval = FALSE.   
      END. 
   END.   
   FOR EACH kon_display WHERE NO-LOCK:
      FIND FIRST innehall WHERE innehall.BERNR = INTEGER(kon_display.BERAONR) AND innehall.OMRADE = kon_display.OMRADE AND innehall.NUM = kon_display.NUM NO-LOCK NO-ERROR.
      IF AVAILABLE innehall THEN DO:
         ASSIGN
         innehall.F1 = kon_display.F1
         innehall.F2 = kon_display.F2
         innehall.F3 = kon_display.F3
         innehall.F4 = kon_display.F4
         innehall.F5 = kon_display.F5
         innehall.F6 = kon_display.F6.  
      END.   
   END. 
   
   FOR EACH fri_temp WHERE NO-LOCK:
      FOR EACH innehall WHERE innehall.BERNR = INTEGER(fri_temp.AONR) AND innehall.OMRADE = fri_temp.OMRADE AND innehall.NUM = fri_temp.NUM NO-LOCK:
         IF fri_temp.SKAP NE "" THEN innehall.F1 = fri_temp.SKAP.
      END.
   END.
END PROCEDURE.

