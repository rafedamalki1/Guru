PROCEDURE Jmfskapa_UI :    
   IF BERVAL.KONSKOD = 0 THEN DO:
      FIND FIRST grundmtrl_temp WHERE grundmtrl_temp.ENR = MTRLBER.ENR AND
      grundmtrl_temp.LEVKOD = MTRLBER.LEVKOD AND grundmtrl_temp.NUM = BERVAL.NUM
     NO-LOCK NO-ERROR.
   END.
   ELSE DO:
      FIND FIRST grundmtrl_temp WHERE grundmtrl_temp.ENR = MTRLBER.ENR AND
      grundmtrl_temp.LEVKOD = MTRLBER.LEVKOD AND grundmtrl_temp.NUM = BERVAL.NUM
     
      NO-LOCK NO-ERROR.
   END.   
   IF NOT AVAILABLE grundmtrl_temp THEN DO:
      CREATE grundmtrl_temp.
      BUFFER-COPY MTRLBER TO grundmtrl_temp.
      ASSIGN 
      grundmtrl_temp.ENHET = LC(grundmtrl_temp.ENHET)
      grundmtrl_temp.NUM = BERVAL.NUM.
   END.
   ELSE DO:
      grundmtrl_temp.ANTAL = grundmtrl_temp.ANTAL + MTRLBER.ANTAL.   
   END.      
END PROCEDURE. 
PROCEDURE JmfGrundmtrl_UI :
   FOR EACH mtrl_temp WHERE  NO-LOCK BY mtrl_temp.NUM BY mtrl_temp.ENR:
      FIND FIRST grundmtrl_temp WHERE grundmtrl_temp.NUM = mtrl_temp.NUM AND grundmtrl_temp.ENR = mtrl_temp.ENR NO-LOCK NO-ERROR.
      IF AVAILABLE grundmtrl_temp THEN DO:
         mtrl_temp.ANTAL = mtrl_temp.ANTAL - grundmtrl_temp.ANTAL.
         IF mtrl_temp.ANTAL = 0 THEN DELETE mtrl_temp.
         DELETE grundmtrl_temp.
      END.   
   END.
   FOR EACH grundmtrl_temp WHERE NO-LOCK BY grundmtrl_temp.NUM BY grundmtrl_temp.ENR:
      FIND FIRST mtrl_temp WHERE mtrl_temp.NUM = grundmtrl_temp.NUM AND mtrl_temp.ENR = grundmtrl_temp.ENR NO-LOCK NO-ERROR.
      IF AVAILABLE mtrl_temp THEN DO:
         mtrl_temp.ANTAL = mtrl_temp.ANTAL - grundmtrl_temp.ANTAL.
         IF mtrl_temp.ANTAL = 0 THEN DELETE mtrl_temp.
         DELETE grundmtrl_temp.
      END.  
      ELSE DO:
         
         CREATE mtrl_temp.
         BUFFER-COPY grundmtrl_temp EXCEPT ANTAL TO mtrl_temp .
         mtrl_temp.antal = grundmtrl_temp.ANTAL * -1.
         
      END.    
   END.
END PROCEDURE.

