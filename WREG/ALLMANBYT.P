DynWp = "DynTableU" + STRING(TIME).
CREATE WIDGET-POOL STRING(DynWp) NO-ERROR.
DEFINE VARIABLE okstarta AS LOGICAL NO-UNDO.
/*
DEFINE VARIABLE  gfalt AS CHARACTER NO-UNDO.   /*fält för gamla värden*/
DEFINE VARIABLE nyfalt AS CHARACTER NO-UNDO.   /*fält för nya värden*/
DEFINE VARIABLE styrtab AS CHARACTER NO-UNDO.  /*tabell som inne håller gamla och nya värden*/
DEFINE VARIABLE updateorgtab AS LOGICAL NO-UNDO. /*om även styrtabellen ska uppdateras*/



gfalt = "LEVKOD" .  /*fält för gamla värden*/
nyfalt = "LEVORT".   /*fält för nya värden*/
styrtab = "LEVERANTOR".  /*tabell som inne håller gamla och nya värden*/
updateorgtab = TRUE.

gfalt = "OMRADE" .  /*fält för gamla värden*/
nyfalt = "TELJOUR".   /*fält för nya värden*/
