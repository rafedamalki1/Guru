PROCEDURE bort_UI :
/* -----------------------------------------------------------
  Purpose:     
  Parameters:  <none>
  Notes:       
-------------------------------------------------------------*/
  MESSAGE "VILL DU VERKLIGEN TA BORT DETTA AVTAL?"
  VIEW-AS ALERT-BOX QUESTION BUTTONS YES-NO TITLE FILL-IN-BORTAV
  UPDATE answer AS LOGICAL.
  IF answer THEN DO TRANSACTION: 
   FOR EACH TRAAVTAB WHERE TRAAVTAB.TRAAVTAL = FILL-IN-BORTAV:
     DELETE TRAAVTAB. 
   END.
   FOR EACH AVDRAGMALTID WHERE AVDRAGMALTID.TRAAVTAL = FILL-IN-BORTAV:
     DELETE AVDRAGMALTID. 
   END.
   FOR EACH KFORMAN WHERE KFORMAN.TRAAVTAL = FILL-IN-BORTAV:
      DELETE KFORMAN.
   END.
   FOR EACH LONFLER WHERE LONFLER.TRAAVTAL = FILL-IN-BORTAV:
      DELETE LONFLER.
      
   END.
   FOR EACH MALTAB WHERE MALTAB.TRAAVTAL = FILL-IN-BORTAV:
      DELETE MALTAB.
   END.
   FOR EACH TRAKTASTART WHERE TRAKTASTART.TRAAVTAL = FILL-IN-BORTAV:
      DELETE TRAKTASTART.
   END.
   FOR EACH TRAKTATAB WHERE TRAKTATAB.TRAAVTAL = FILL-IN-BORTAV:
      DELETE TRAKTATAB.
   END.
   FOR EACH TRAKTFLER WHERE TRAKTFLER.TRAAVTAL = FILL-IN-BORTAV:
      DELETE TRAKTFLER.
   END.
   FOR EACH TRAKTREGLER WHERE TRAKTREGLER.TRAAVTAL = FILL-IN-BORTAV:
      DELETE TRAKTREGLER.
   END.

   
END.
END PROCEDURE.

