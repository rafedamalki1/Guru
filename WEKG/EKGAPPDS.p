PROCEDURE Dubbjmf_UI :
   DEFINE INPUT  PARAMETER jmfstr AS CHARACTER NO-UNDO.
   DEFINE INPUT  PARAMETER Tabnamn AS CHARACTER NO-UNDO.
   DEFINE VARIABLE jmbuffh AS HANDLE NO-UNDO.
   CREATE BUFFER jmbuffh FOR TABLE Tabnamn.
   DO TRANSACTION:
      jmbuffh:FIND-UNIQUE(jmfstr,NO-LOCK) NO-ERROR.
      IF jmbuffh:AMBIGUOUS = TRUE THEN DO:
         jmbuffh:FIND-FIRST(jmfstr,EXCLUSIVE-LOCK) NO-ERROR. 
         jmbuffh:BUFFER-DELETE().  
      END.
   END.   
END PROCEDURE.


