   DEF VAR AA AS DECIMAL.
   FOR EACH SUMTID WHERE SUMTID.AONR = "151032" AND SUMTID.DELNR = 0 AND  
      YEAR(SUMTID.DATUM) = 2001:
      DELETE SUMTID.
   END.
   FOR EACH SUMTIDDAG WHERE SUMTIDDAG.AONR = "151032" AND SUMTIDDAG.DELNR = 0
   AND SUMTIDDAG.VECKOKORD NE "" AND  YEAR(SUMTIDDAG.DATUM) = 2001
   BREAK  BY SUMTIDDAG.PERSONALKOD BY  
   SUMTIDDAG.AONR BY SUMTIDDAG.DELNR BY SUMTIDDAG.PRISTYP:       
      ACCUMULATE SUMTIDDAG.LONKOST (TOTAL BY SUMTIDDAG.PERSONALKOD BY SUMTIDDAG.AONR BY SUMTIDDAG.DELNR BY SUMTIDDAG.PRISTYP). 
      ACCUMULATE SUMTIDDAG.OBELOPP (TOTAL BY SUMTIDDAG.PERSONALKOD BY SUMTIDDAG.AONR BY SUMTIDDAG.DELNR BY SUMTIDDAG.PRISTYP). 
      ACCUMULATE SUMTIDDAG.BELOPP (TOTAL BY SUMTIDDAG.PERSONALKOD BY SUMTIDDAG.AONR BY SUMTIDDAG.DELNR BY SUMTIDDAG.PRISTYP). 
      ACCUMULATE SUMTIDDAG.TBELOPP (TOTAL BY SUMTIDDAG.PERSONALKOD BY SUMTIDDAG.AONR BY SUMTIDDAG.DELNR BY SUMTIDDAG.PRISTYP). 
      ACCUMULATE SUMTIDDAG.IKOST (TOTAL BY SUMTIDDAG.PERSONALKOD BY SUMTIDDAG.AONR BY SUMTIDDAG.DELNR BY SUMTIDDAG.PRISTYP). 
      ACCUMULATE SUMTIDDAG.TIMMAR (TOTAL BY SUMTIDDAG.PERSONALKOD BY SUMTIDDAG.AONR BY SUMTIDDAG.DELNR BY SUMTIDDAG.PRISTYP). 
      ACCUMULATE SUMTIDDAG.OTIMMAR (TOTAL BY SUMTIDDAG.PERSONALKOD BY SUMTIDDAG.AONR BY SUMTIDDAG.DELNR BY SUMTIDDAG.PRISTYP). 
      IF LAST-OF(SUMTIDDAG.PRISTYP) THEN DO:     
         DO TRANSACTION:
            FIND FIRST SUMTID WHERE SUMTID.DATUM = 01/01/2001 AND 
            SUMTID.VECKOKORD NE "" AND 
            SUMTID.PERSONALKOD = SUMTIDDAG.PERSONALKOD AND
            SUMTID.AONR = SUMTIDDAG.AONR AND SUMTID.DELNR = SUMTIDDAG.DELNR AND 
            SUMTID.PRISTYP = SUMTIDDAG.PRISTYP 
            EXCLUSIVE-LOCK NO-ERROR.       
            IF NOT AVAILABLE SUMTID THEN DO:
               CREATE SUMTID.          
            END.
            ASSIGN               
            SUMTID.OMRADE = SUMTIDDAG.OMRADE 
            SUMTID.VECKOKORD = "KLAR"                             
            SUMTID.PERSONALKOD = SUMTIDDAG.PERSONALKOD               
            SUMTID.DATUM = DATE(01,01,YEAR(SUMTIDDAG.DATUM)) 
            SUMTID.AUTODATUM = TODAY
            SUMTID.AONR = SUMTIDDAG.AONR   
            SUMTID.DELNR = SUMTIDDAG.DELNR
            SUMTID.PRISTYP = SUMTIDDAG.PRISTYP 
            SUMTID.FASTAAONR = SUMTIDDAG.FASTAAONR          
            SUMTID.GEOMRADE = SUMTIDDAG.OMRADE 
            SUMTID.ORT = SUMTIDDAG.ORT
            SUMTID.FORNAMN = SUMTIDDAG.FORNAMN 
            SUMTID.EFTERNAMN = SUMTIDDAG.EFTERNAMN
            SUMTID.OMRADE = SUMTIDDAG.OMRADE
            SUMTID.BEFATTNING = SUMTIDDAG.BEFATTNING
            SUMTID.PERSMASK = SUMTIDDAG.PERSMASK  
            SUMTID.PRISI = SUMTIDDAG.PRISI
            SUMTID.LONKOST  = SUMTID.LONKOST + (ACCUM TOTAL BY SUMTIDDAG.PRISTYP SUMTIDDAG.LONKOST)                         
            SUMTID.OBELOPP = SUMTID.OBELOPP + (ACCUM TOTAL BY SUMTIDDAG.PRISTYP SUMTIDDAG.OBELOPP)
            SUMTID.BELOPP = SUMTID.BELOPP + (ACCUM TOTAL BY SUMTIDDAG.PRISTYP SUMTIDDAG.BELOPP)
            SUMTID.TBELOPP = SUMTID.TBELOPP + (ACCUM TOTAL BY SUMTIDDAG.PRISTYP SUMTIDDAG.TBELOPP)              
            SUMTID.IKOSTNAD = SUMTID.IKOSTNAD + (ACCUM TOTAL BY SUMTIDDAG.PRISTYP SUMTIDDAG.IKOST).               
            SUMTID.TIMMAR = SUMTID.TIMMAR + (ACCUM TOTAL BY SUMTIDDAG.PRISTYP SUMTIDDAG.TIMMAR).              
            SUMTID.OTIMMAR = SUMTID.OTIMMAR + (ACCUM TOTAL BY SUMTIDDAG.PRISTYP SUMTIDDAG.OTIMMAR).              
            AA = AA + SUMTID.LONKOST +      
                  SUMTID.OBELOPP +
                  SUMTID.TBELOPP +
                      SUMTID.IKOSTNAD +
                      SUMTID.TIMMAR +
                      SUMTID.OTIMMAR.

   DISPLAY AA.
         END.
      END.
   END.
