FOR EACH AVDELNING WHERE AVDELNING.KOSTMASK = 1:
   DISP AVDELNING.AVDELNINGNAMN AVDELNING.AVDELNINGNR AVDELNING.KOSTMASK.
   FOR EACH STORDISTRIKT WHERE STORDISTRIKT.AVDELNINGNR = AVDELNING.AVDELNINGNR:
