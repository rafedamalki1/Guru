      IF sortkollvar = "" or sortkollvar = ? THEN RUN setcolindex_UI IN brwproc[13] (INPUT "").    
   END.
   ELSE DO:
      MESSAGE "Du har inte valt något kopplat materiel som Lin/Kab ska ändras för."
      VIEW-AS ALERT-BOX TITLE "Meddelande".
   END.      
END PROCEDURE.

