DEFINE VAR PKOD LIKE TIDREGITAB.PERSONALKOD.
DEFINE VAR RDATUM LIKE TIDREGITAB.DATUM.
DEFINE VAR RSTART LIKE TIDREGITAB.START.
DEFINE VAR RSLUT LIKE TIDREGITAB.SLUT.
FOR EACH TIDREGITAB WHERE VECKOKORD = "" AND PRISTYP = "RESTID..."
BY PERSONALKOD BY DATUM BY START BY SLUT:
IF PKOD = TIDREGITAB.PERSONALKOD AND
   RDATUM = TIDREGITAB.DATUM AND
   RSTART = TIDREGITAB.START AND 
   RSLUT = TIDREGITAB.SLUT THEN  DISPLAY PERSONALKOD DATUM START SLUT.
   ASSIGN
   PKOD = TIDREGITAB.PERSONALKOD 
   RDATUM = TIDREGITAB.DATUM
   RSTART = TIDREGITAB.START
   RSLUT = TIDREGITAB.SLUT.  
MESSAGE PERSONALKOD DATUM START SLUT.
END.   
