/*BERBILD.I*/

DEFINE {&NEW} {&SHARED} TEMP-TABLE berbildtemp  
   FIELD KTYPKOD AS CHARACTER FORMAT "X(8)" LABEL "Konstruktionskod"
   FIELD HJKONSTGRUPPB AS CHARACTER
   FIELD HJUNDERGRUPPB AS CHARACTER
   FIELD FILNAMN AS CHARACTER FORMAT "X(25)" LABEL "Filnamn"
   FIELD ORDNING AS INTEGER FORMAT 9 INITIAL 0 LABEL "Ordning"          
   FIELD BBROW AS ROWID
   INDEX KOD IS PRIMARY KTYPKOD ORDNING
   INDEX HJ HJKONSTGRUPPB HJUNDERGRUPPB KTYPKOD.
