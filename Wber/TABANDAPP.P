/*TABANDAPP.P*/

PROCEDURE btnok_UI :
   DEFINE INPUT PARAMETER tabvar1 AS CHARACTER NO-UNDO.
   DEFINE INPUT PARAMETER tabvar2 AS CHARACTER NO-UNDO.
   DEFINE INPUT PARAMETER tabvar3 AS CHARACTER NO-UNDO.
   DEFINE OUTPUT PARAMETER felmedd AS CHARACTER NO-UNDO.
   felmedd = "".
   DO TRANSACTION:
      FIND FIRST ANNNAMN EXCLUSIVE-LOCK NO-ERROR.
      IF AVAILABLE ANNNAMN THEN DO:
         ASSIGN
         ANNNAMN.TAB1 = tabvar1 
         ANNNAMN.TAB2 = tabvar2 
         ANNNAMN.TAB3 = tabvar3. 
      END.
      ELSE DO:
         ASSIGN felmedd = "Det gick inte att spara, kontakta Elpool.".
         RETURN.
      END.
   END.
   RELEASE ANNNAMN NO-ERROR.
END PROCEDURE.
