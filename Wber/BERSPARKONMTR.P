         ASSIGN
         BERORD.OMRADE = valomrade
         BERORD.AONR = valaonr
         BERORD.NUM = kon_val.NUM
         BERORD.ORD = kon_val.ORD.
      /*END.*/
   END.
   
   RELEASE BERORD NO-ERROR.
