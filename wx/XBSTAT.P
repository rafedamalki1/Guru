   DEFINE BUFFER A FOR BESTSTAT.
   DEFINE BUFFER B FOR BESTSTAT.
   FOR EACH BESTSTAT WHERE DATUM GE 01/01/2008 AND BESTSTAT.BERNR > 0 NO-LOCK BY DATUM:
      FIND FIRST A WHERE A.OMRADE = BESTSTAT.OMRADE  AND A.BERNR = BESTSTAT.BERNR AND RECID(A) NE RECID(BESTSTAT) 
      AND A.BESTALLD = BESTSTAT.BESTALLD AND A.LEVKOD = BESTSTAT.LEVKOD AND A.BESTNR = BESTSTAT.BESTNR NO-LOCK NO-ERROR.
      IF AVAILABLE A THEN DO:
         FIND FIRST B WHERE B.OMRADE = A.OMRADE  AND B.BERNR = A.BERNR AND RECID(B) NE RECID(A ) 
         AND B.BESTALLD NE A.BESTALLD AND B.LEVKOD = A.LEVKOD AND B.BESTNR = A.BESTNR NO-LOCK NO-ERROR.
         IF NOT AVAILABLE B THEN DO:

            /*DISP BESTSTAT.BERNR BESTSTAT.OMRADE BESTSTAT.BESTNR BESTSTAT.BESTALLD BESTSTAT.DATUM BESTSTAT.ANVANDARE.*/
            DISP BESTSTAT WITH FRAME CC.
            DISP A WITH FRAME DD.
         END.
      END.
   
END.
