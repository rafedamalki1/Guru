   /*DELALLBER.P*/
   /*aonr bort mm*/
   OPEN QUERY DQ FOR EACH BEREDNING NO-LOCK.
   DO TRANSACTION:
      GET FIRST DQ EXCLUSIVE-LOCK.
      RUN DELBERE.P
      (INPUT BEREDNING.BERAONR, INPUT BEREDNING.OMRADE).
      DELETE BEREDNING.
   END.
   REPEAT :
      DO TRANSACTION:
         GET NEXT DQ EXCLUSIVE-LOCK.
         IF AVAILABLE BEREDNING THEN DO:
            RUN DELBERE.P
            (INPUT BEREDNING.BERAONR, INPUT BEREDNING.OMRADE).
            DELETE BEREDNING.
         END.
         ELSE LEAVE.
      END.
   END.
   CLOSE QUERY DQ.
