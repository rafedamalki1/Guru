FOR EACH BERMTRL WHERE BERMTRL.OMRADE = "TEV7" AND BERMTRL.AONR = "1086" BY BERMTRL.DATUM BY BERMTRL.ENR:
   DISP BERMTRL.DATUM BERMTRL.INKOP BERMTRL.KLAR BERMTRL.ENR BERMTRL.ANTAL BERMTRL.LEVKOD.
END.
FOR EACH BESTSTAT WHERE BESTSTAT.OMRADE = "TEV7" AND BESTSTAT.BERNR = 1086:
   DISP BESTSTAT.
END.
FOR EACH LEVTRP WHERE LEVTRP.OMRADE = "TEV7" AND LEVTRP.BERNR = 1086:
   DISP LEVTRP WITH 2 COL.
   DISP LEVTRP.L1 FORMAT "X(15)".
END.
FOR EACH LEVTRP2 WHERE LEVTRP2.OMRADE = "TEV7" AND LEVTRP2.BERNR = 1086:
   DISP LEVTRP2.
END.
