OUTPUT TO C:\FLKOLL.TXT.
FOR EACH PERSONALTAB WHERE PERSONALTAB.AKTIV = TRUE  NO-LOCK:   
   FIND FIRST FLEXAVT WHERE FLEXAVT.PERSONALKOD = PERSONALTAB.PERSONALKOD AND FLEXAVT.FLEXTID = TRUE NO-LOCK NO-ERROR.
   IF AVAILABLE FLEXAVT THEN DO:
      FIND FIRST FSALDMAN WHERE FSALDMAN.PERSONALKOD = PERSONALTAB.PERSONALKOD AND FSALDMAN.DATUM = 01/31/2011 NO-LOCK NO-ERROR.
      IF NOT AVAILABLE FSALDMAN THEN DO:
         DISP PERSONALTAB.PERSONALKOD "01/31/2011".
      END.
      FIND FIRST FSALDMAN WHERE FSALDMAN.PERSONALKOD = PERSONALTAB.PERSONALKOD AND FSALDMAN.DATUM = 12/31/2010 NO-LOCK NO-ERROR.
      IF NOT AVAILABLE FSALDMAN THEN DO:
         DISP PERSONALTAB.PERSONALKOD "12/31/2010".
      END.
   END.
   
END.
