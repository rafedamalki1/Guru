/*VAR IFRÅN SKALL FILERNA TAS*/ 
IF OPSYS = "UNIX" THEN DO: 
   kommandoprog = "/u10/guru/import/aoin.txt".
   kommando = "ls /u10/guru/import/ao*.old > /u10/guru/import/aoin.txt".   
END.    
OS-DELETE VALUE(kommandoprog).
OS-COMMAND SILENT VALUE(kommando) . 
/*VILKA FILER*/
INPUT FROM VALUE(kommandoprog) NO-ECHO.
REPEAT:
   DO TRANSACTION: 
      CREATE infil.
      ASSIGN.
      IMPORT infil   NO-ERROR.
   END.
END.
INPUT CLOSE.
FOR EACH infil:   
   IF INDEX(infil.PROGNAMN,".old") = 0 THEN DO:       
      DELETE infil.
      NEXT.
   END.
   infil.PROGNAMN = SUBSTRING(infil.PROGNAMN,1,INDEX(infil.PROGNAMN,".old") - 1).   
END.
FOR EACH infil:  
      prognamnold = infil.PROGNAMN + ".old".                      
      prognamnque = infil.PROGNAMN + ".q".
     
   IF INDEX(prognamnold,"u10") > 0 THEN prognamnold2 = REPLACE(prognamnold,"u10","u12").
   display prognamnold prognamnold2 with frame cc width 78.
   message prognamnold prognamnold2 INDEX(prognamnold,"u10").
   OS-copy VALUE(prognamnold) VALUE(prognamnold2).
   CC = OS-ERROR.
   DISPLAY CC WITH FRAME CCC.
   OS-DELETE VALUE(prognamnque). 
END.
