/*KALKBEJPERSIST.P*/

PROCEDURE kbefbort_UI :
   DEFINE INPUT PARAMETER valaonr AS CHARACTER NO-UNDO.
   DEFINE INPUT PARAMETER valomrade AS CHARACTER NO-UNDO.
   DEFINE INPUT PARAMETER numvar AS INTEGER NO-UNDO.   
   FOR EACH BERKALK WHERE BERKALK.AONR = valaonr AND
      BERKALK.OMRADE = valomrade AND BERKALK.NUM = numvar EXCLUSIVE-LOCK:
      DELETE BERKALK.      
   END.
END PROCEDURE.
