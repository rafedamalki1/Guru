/*XRENSTID.P*/
DEFINE QUERY sumq FOR SUMTID. 
DEFINE QUERY sumdq FOR SUMTIDDAG.  
DEFINE QUERY tidq FOR TIDREGITAB.
OPEN QUERY sumq FOR EACH SUMTID EXCLUSIVE-LOCK.    
DO TRANSACTION:   
   GET FIRST sumq EXCLUSIVE-LOCK.
   IF AVAILABLE SUMTID THEN DO:
      DELETE SUMTID.
   END. 
END.             
REPEAT TRANSACTION:                    
   GET NEXT sumq EXCLUSIVE-LOCK.          
   IF NOT AVAILABLE SUMTID THEN LEAVE.
   ELSE DO :
      DELETE SUMTID.      
   END.                
END.  
CLOSE QUERY sumdq.  
OPEN QUERY sumdq FOR EACH SUMTIDDAG EXCLUSIVE-LOCK.    
DO TRANSACTION:   
   GET FIRST sumdq EXCLUSIVE-LOCK.
   IF AVAILABLE SUMTIDDAG THEN DO:
      DELETE SUMTIDDAG.
   END. 
END.             
REPEAT TRANSACTION:                    
   GET NEXT sumdq EXCLUSIVE-LOCK.          
   IF NOT AVAILABLE SUMTIDDAG THEN LEAVE.
   ELSE DO :
      DELETE SUMTIDDAG.      
   END.                
END.  
CLOSE QUERY sumdq. 
OPEN QUERY tidq FOR EACH TIDREGITAB EXCLUSIVE-LOCK.    
DO TRANSACTION:   
   GET FIRST tidq EXCLUSIVE-LOCK.
   IF AVAILABLE TIDREGITAB THEN DO:
      DELETE TIDREGITAB.
   END. 
END.             
REPEAT TRANSACTION:                    
   GET NEXT tidq EXCLUSIVE-LOCK.          
   IF NOT AVAILABLE TIDREGITAB THEN LEAVE.
   ELSE DO :
      DELETE TIDREGITAB.      
   END.                
END.  
CLOSE QUERY tidq. 
