         FILL-IN-TAB1:HIDDEN = TRUE 
         FILL-IN-TAB2:HIDDEN = TRUE 
         BTN_ANL:HIDDEN = TRUE
         FILL-IN-NR1:HIDDEN = TRUE
         FILL-IN-NR2:HIDDEN = TRUE.         
      END.      
   END.
