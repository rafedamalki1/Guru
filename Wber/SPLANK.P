/*SPLANK.P*/


PROCEDURE checklank_UI:
   DEFINE INPUT PARAMETER valenr AS CHARACTER NO-UNDO.
   DEFINE  INPUT PARAMETER levnamnvar AS CHARACTER NO-UNDO.
   DEFINE OUTPUT PARAMETER lankst AS CHARACTER NO-UNDO.
   FIND FIRST LEVERANTOR WHERE LEVERANTOR.LEVNAMN = levnamnvar NO-LOCK NO-ERROR.
   IF AVAILABLE LEVERANTOR THEN DO:
      FIND FIRST ANSVARIGTAB WHERE ANSVARIGTAB.PERSONALKOD = valenr AND ANSVARIGTAB.FORNAMN = LEVERANTOR.LEVKOD AND ANSVARIGTAB.OMREGTID = 00.15 NO-LOCK NO-ERROR.
      IF AVAILABLE ANSVARIGTAB THEN DO:
         lankst = ANSVARIGTAB.EFTERNAMN.
      END.
   END.
END PROCEDURE.

PROCEDURE checklankbild_UI:
   DEFINE INPUT PARAMETER valenr AS CHARACTER NO-UNDO.
   DEFINE INPUT PARAMETER levnamnvar AS CHARACTER NO-UNDO.
   DEFINE OUTPUT PARAMETER lankst AS CHARACTER NO-UNDO.
   FIND FIRST LEVERANTOR WHERE LEVERANTOR.LEVNAMN = levnamnvar NO-LOCK NO-ERROR.
   IF AVAILABLE LEVERANTOR THEN DO:
      FIND FIRST ANSVARIGTAB WHERE ANSVARIGTAB.PERSONALKOD = valenr AND ANSVARIGTAB.FORNAMN = LEVERANTOR.LEVKOD AND ANSVARIGTAB.OMREGTID = 2 NO-LOCK NO-ERROR.
      IF AVAILABLE ANSVARIGTAB THEN DO:
         lankst = ANSVARIGTAB.EFTERNAMN.
      END.
   END.
END PROCEDURE.

