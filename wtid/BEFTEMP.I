   FIELD PERSMASK      AS LOGICAL
   FIELD BRAVO         AS LOGICAL
   FIELD AV-LEVEL      AS INTEGER
   FIELD KOSTNADSSLAG  AS CHARACTER
   FIELD PRISA         AS DECIMAL
   FIELD BILF          AS LOGICAL
   FIELD PLUSD         AS LOGICAL
   FIELD NAMN          AS CHARACTER LABEL "Befattning"
